{{- $.Scratch.Add "index" slice -}}
{{- range where .Site.Pages "Type" "in"  (slice "post") -}}
{{- $.Scratch.Add "index" (dict "uri" .Permalink "title" .Title "day" .Params.day "timeStart" .Params.timeStart "timeEnd" .Params.timeEnd "summary" .Summary "description" .Description) -}}
{{- end -}}
{{- $.Scratch.Get "index" | jsonify -}}